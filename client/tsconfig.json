{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "baseUrl": "..",
    "paths": {
      "@definitions/*": [
        "definitions/*"
      ],
      "@app/*": [
        "client/src/*"
      ]
    }
  },
  "include": [
    "./src/**/*",
    "../definitions/*"
  ]
}
